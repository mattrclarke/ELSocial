<div class="container">
  <div class="row">
    <div class="col-md-6">
      <h1 class="mt-3">Viewing <%= @profile.full_name %></h1>
      <div>
        <%= image_tag @profile.image_url.to_s, class: "profile-image" %>
      <div id="follow-details" class="">
        <%= render 'shared/follow_details', other_user: @other_user %>
      </div>
        <h3>About me</h3>
        <%= @profile.about_me %>
      </div>
    </div>
  </div>
</div>
