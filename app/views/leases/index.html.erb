<p id="notice"><%= notice %></p>
    <h1>Leases</h1>
    <table>
      <thead>
        <tr>
          <th>Location</th>
          <th>Latitude</th>
          <th>Longitude</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @leases.each do |lease| %>
          <tr>
            <td><%= lease.location %></td>
            <td><%= lease.latitude %>t</td>
            <td><%= lease.longitude %></td>
            <td><%= link_to 'Show', lease %></td>
            <td><%= link_to 'Edit', edit_lease_path(lease) %></td>
            <td><%= link_to 'Destroy', lease, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>

    <br>

    <%= link_to 'New Lease', new_lease_path %>
